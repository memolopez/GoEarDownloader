{
  "manifest_version": 2,

  "name": "GoEarDownloader",
  "description": "This extension gives you a link to download the track you are listening.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://www.goear.com/listen/*"],
      "css": ["style.css"],
      "js": ["jquery-2.1.0.min.js", "master.js"]
    }
  ],

  "web_accessible_resources": [
  "jquery-1.10.1.min.map"
  ]
}